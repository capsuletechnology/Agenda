<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Agenda.View.CadastroView">
  <StackLayout>
    <Entry Text="{Binding Nome}" Keyboard="Text" Placeholder="Nome"></Entry>
    <Entry Text="{Binding Telefone}" Keyboard="Telephone" Placeholder="(00)00000-0000"></Entry>
    <Entry Text="{Binding Email}" Keyboard="Email" Placeholder="xxxx@xx.com.br"></Entry>
    <Button Text="Cadastrar" Command="{Binding CadastrarCommand}"></Button>
  </StackLayout>
</ContentPage>